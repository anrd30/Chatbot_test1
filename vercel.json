{
  "version": 2,
  "builds": [
    {
      "src": "api/chat/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend_new/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend_new/$1"
    }
  ]
}
