{
  "version": 2,
  "builds": [
    {
      "src": "chat/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/chat/index.py",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": ["*"],
        "Access-Control-Allow-Methods": ["POST", "OPTIONS"],
        "Access-Control-Allow-Headers": ["Content-Type"],
        "Access-Control-Allow-Credentials": ["true"]
      }
    },
    {
      "src": "/api/(.*)",
      "status": 404
    }
  ]
}
